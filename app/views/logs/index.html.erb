<div class="container">

  <p id="notice"><%= notice %></p>

  <h1>Logs</h1>
  <h6>only the last one will use as a data feed</h6>
  <%= link_to 'New Data Feed', new_log_path %>
  <hr>
  <table>
    <thead>
      <tr>
        <th>Data feed</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @logs.each do |log| %>
        <tr>
          <td><%= log.name %></td>
          <td><%= link_to 'Show', log %></td>
          <%# <td>= link_to 'Edit', edit_log_path(log)</td> %>
          <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
